{
  "name": "back-express-template",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "build": "npx tsc",
    "dev": "nodemon",
    "format:fix": "prettier --ignore-path .prettierignore --write .",
    "format:check": "prettier --ignore-path .prettierignore --check .",
    "format:watch": "onchange \"**/*\" -- prettier --write --ignore-path .prettierignore --ignore-unknown {{changed}}",
    "lint:fix": "eslint . -c eslint.config.mjs --fix",
    "lint:check": "eslint . -c eslint.config.mjs --no-fix",
    "test": "vitest run",
    "test:watch": "vitest",
    "coverage": "vitest run --coverage",
    "coverage:show": "http-server -o ./coverage/index.html -p 6001",
    "coverage:show-lcov": "http-server -o ./coverage/index.html -p 6001",
    "coverage:verify-lcov": "lcov-total ./coverage/lcov.info --gte=10",
    "prepare": "husky"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "packageManager": "pnpm@9.1.1+sha512.14e915759c11f77eac07faba4d019c193ec8637229e62ec99eefb7cf3c3b75c64447882b7c485142451ee3a6b408059cdfb7b7fa0341b975f12d0f7629c71195",
  "dependencies": {
    "dotenv": "16.4.5",
    "express": "4.19.2"
  },
  "devDependencies": {
    "@eslint/js": "9.9.0",
    "@types/eslint__js": "8.42.3",
    "@types/express": "4.17.21",
    "@vitest/coverage-v8": "2.0.5",
    "eslint": "9.9.0",
    "http-server": "14.1.1",
    "husky": "9.1.4",
    "lcov-total": "2.1.1",
    "onchange": "7.1.0",
    "prettier": "3.3.3",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.5.4",
    "typescript-eslint": "8.0.1",
    "vitest": "2.0.5"
  }
}
